{
	"extends": ["airbnb", "airbnb/hooks", "next/core-web-vitals", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": ["error"]
	},
	"settings": {
		"import/resolver": {
			"alias": {
				"map": [
					["@", "./src"] // '@'를 'src' 폴더로 매핑
				],
				"extensions": [".js", ".jsx", ".ts", ".tsx", ".json"]
			}
		}
	}
}
